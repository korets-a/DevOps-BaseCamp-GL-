pipeline {
    agent any
    stages {
        stage('---update---') {
            steps {
                sh 'apt update '
            }
        }
        stage('--install--') {
            steps {
               sh ' echo " install "'
            }
        }
        stage('--run--') {
            steps {
               sh ' echo " run "'
            }
        }
        stage('--status--') {
            steps {
                sh 'echo " status "'
            }
        }
    }
}
