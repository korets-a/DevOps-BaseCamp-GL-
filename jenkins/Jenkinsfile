pipeline {
    agent any
    stages {
        stage('---update---') {
            steps {
                apt update
            }
        }
        stage('--install--') {
            steps {
                echo " test "
            }
        }
        stage('--run--') {
            steps {
                echo " package "
            }
        }
        stage('--status--') {
            steps {
                echo " test "
            }
        }
    }
}
